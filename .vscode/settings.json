{
  "files.associations": {
    "*.scm": "scheme",
    "*.sld": "scheme", 
    "*.rkt": "racket",
    "*.sls": "scheme",
    "*.lisp": "lisp"
  },
  "files.exclude": {
    "**/*.fasl": true,
    "**/*.so": true,
    "**/build": true,
    "**/dist": true,
    "**/.devbox": true,
    "**/*.log": true
  },
  "search.exclude": {
    "**/build": true,
    "**/dist": true,
    "**/*.fasl": true,
    "**/*.so": true
  },
  "editor.formatOnSave": true,
  "editor.insertFinalNewline": true,
  "editor.trimTrailingWhitespace": true,
  "editor.rulers": [80, 100],
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "[scheme]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.autoIndent": "full"
  },
  "[racket]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.autoIndent": "full"
  },
  "[lisp]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.autoIndent": "full"
  },
  "terminal.integrated.env.osx": {
    "ATHENA_DEV": "1"
  },
  "terminal.integrated.env.linux": {
    "ATHENA_DEV": "1"
  },
  "tasks.version": "2.0.0",
  "extensions.recommendations": [
    "magic-racket.racket-repl",
    "codejag.lisp-paredit",
    "ms-vscode.makefile-tools"
  ]
}